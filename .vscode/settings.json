{
    "latex-workshop.latex.outDir": "%DIR%/output",
    "latex-workshop.latex.tools": [
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-output-directory=output",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "output/%DOCFILE%"
            ]
        },
        {
            "name": "movepdf",
            "command": "cp",
            "args": [
                "output/%DOCFILE%.pdf",
                "%DIR%/Praca InÅ¼ynierska.pdf"
            ]
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "pdflatex -> bibtex -> pdflatex*2 -> movepdf",
            "tools": [
                "pdflatex",
                "bibtex",
                "pdflatex",
                "pdflatex",
                "movepdf"
            ]
        }
    ]
}
